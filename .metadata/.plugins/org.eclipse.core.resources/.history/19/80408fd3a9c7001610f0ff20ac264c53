#include <stdio.h>
#include <stdbool.h>

#define ARRAYSIZE 5

int array[ARRAYSIZE] = {4,3,5,2,1};
int *array_ptr = array;

void swap(int a, int b);

int main()
{
	int a = 4;
	int b = 5;
	swap(&a, &b);
	printf("%d\n", a);
	printf("%d\n", b);
	return 0;
	}

void swap(int *a_ptr, int *b_ptr)
{
	int c = *a_ptr;
	*a_ptr = *b_ptr;
	*b_ptr = c;
	}
