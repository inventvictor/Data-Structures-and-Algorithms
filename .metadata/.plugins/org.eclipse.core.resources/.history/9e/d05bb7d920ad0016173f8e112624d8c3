#include <stdio.h>
#include <stdbool.h>

#ifndef ARRAYSTACK_H_
#define ARRAYSTACK_H_

#define MAXSIZE 25

char array[MAXSIZE];
char *array_ptr = &array[MAXSIZE-1];
int pointer_counter = MAXSIZE;

bool stack_isEmpty();
bool stack_isFull();
int stack_peek();
bool stack_push(int data);
int stack_pop();
int stack_size();

#endif /* ARRAYSTACK_H_ */
